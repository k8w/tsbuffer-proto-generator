{
  "name": "tsbuffer-proto-generator",
  "version": "1.3.0",
  "description": "",
  "main": "index.cjs",
  "exports": {
    "require": "./index.cjs",
    "import": "./index.mjs"
  },
  "typings": "./index.d.ts",
  "scripts": {
    "test": "npx mocha",
    "coverage": "nyc mocha test/**/*.test.ts && start coverage\\index.html",
    "build": "npm run build:js && npm run build:dts && npm run build:doc && cp package.json LICENSE README.md dist/",
    "build:js": "rm -rf dist && npx rollup -c",
    "build:dts": "rm -rf lib && npx tsc && npx api-extractor run --local --verbose && node scripts/removePrivate.js && rm -rf lib",
    "build:doc": "rm -rf docs/references && npx api-documenter markdown --input temp --output docs/references"
  },
  "author": "k8w",
  "keywords": [
    "tsbuffer",
    "protobuf",
    "typescript",
    "serialize",
    "json",
    "schema",
    "binary",
    "protocol",
    "websocket",
    "k8w"
  ],
  "license": "ISC",
  "dependencies": {
    "k8w-crypto": "^0.2.0",
    "k8w-extend-native": "^1.4.5",
    "tsbuffer-schema": "^2.0.0",
    "typescript": "^4.2.3"
  },
  "devDependencies": {
    "@microsoft/api-documenter": "^7.12.14",
    "@microsoft/api-extractor": "^7.13.2",
    "@types/mocha": "^5.2.7",
    "@types/node": "^14.14.37",
    "mocha": "^8.3.2",
    "nyc": "^13.3.0",
    "rollup": "^1.32.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.25.3",
    "ts-node": "^8.10.2",
    "tslib": "^2.1.0"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "coverage",
      "test"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  }
}
